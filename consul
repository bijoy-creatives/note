



sudo consul agent -server -bootstrap-expect=1 -data-dir=/var/lib/consul -bind=127.0.0.1 -ui
sudo consul agent -server -bootstrap-expect=1 -data-dir=/var/lib/consul -bind=192.168.1.12 -ui

sudo consul agent -server -bootstrap-expect=1 -data-dir=/var/lib/consul -bind=localhost -ui
sudo consul agent -server -bootstrap-expect=1 -data-dir=/var/lib/consul -bind=192.168.1.12 -ui



sudo systemctl stop consul
sudo pkill -f consul

sudo rm -rf /var/lib/consul
